#%RAML 1.0 DataType
type: object
properties:
  "user":
    required: true
    type: string
    default: "admin"
  "password":
    required: true
    type: string
    default: "admin"
  "accountNum":
    required: true
    type: string
    minLength: 10
    maxLength: 10
  "bank":
    required: true
    enum:
      - ICICI
      - AXIS
      - HDFC
    
